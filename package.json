{
  "name": "json2any",
  "displayName": "json2any",
  "description": "json2css json2dts",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "vscode-plugin-btrip",
      "properties": {
        "vscodePluginBtrip.json2ts.prependWithI": {
          "type": "boolean",
          "default": true,
          "description": "是否给每个类型添加前缀 I"
        },
        "vscodePluginBtrip.json2ts.sortAlphabetically": {
          "type": "boolean",
          "default": false,
          "description": "是否按字母顺序排序属性"
        },
        "vscodePluginBtrip.json2ts.addExport": {
          "type": "boolean",
          "default": false,
          "description": "是否导出每个类型"
        },
        "vscodePluginBtrip.json2ts.useArrayGeneric": {
          "type": "boolean",
          "default": false,
          "description": "是否使用 Array<T> 声明数组类型"
        },
        "vscodePluginBtrip.json2ts.optionalFields": {
          "type": "boolean",
          "default": false,
          "description": "是否将所有属性都置为可选"
        },
        "vscodePluginBtrip.json2ts.prefix": {
          "type": "string",
          "default": "",
          "description": "类型名前缀"
        },
        "vscodePluginBtrip.json2ts.rootObjectName": {
          "type": "string",
          "default": "RootObject",
          "description": "根类型名"
        }
      }
    },
    "commands": [
      {
        "command": "cssobj2css",
        "title": "css-obj to css",
        "icon": "./json2any.png"
      },
      {
        "command": "json2dts",
        "title": "json to dts",
        "icon": "./json2any.png"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "cssobj2css",
          "group": "navigation",
          "when": "resourceLangId == css || resourceLangId == less || resourceLangId == scss"
        },
        {
          "command": "json2dts",
          "group": "navigation",
          "when": "resourceLangId == typescript"
        }
      ]
    },
    "keybindings": {
      "command": "cssobj2css",
      "key": "ctrl+t",
      "mac": "cmd+t",
      "when": "editorTextFocus"
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/postcss-js": "^4.0.0",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.4",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {}
}
